for num in range(1, 101):
    if num >= 11 and num <= 19:
        print(num, 'процентов')
    elif num % 10 == 0 or num % 10 == 5 or num % 10 == 6 or num % 10 == 7 or num % 10 == 8 or num % 10 == 9:
        print(num, 'процентов')
    elif num % 10 == 2 or num % 10 == 3 or num % 10 == 4:
        print(num, 'процента')
    else:
        print(num, 'процент')
